    "@types/node-fetch": "^2.5.7",
    "@types/rewire": "^2.5.28",
    "coveralls": "^3.0.9",
    "jest": "^26.0.1",
    "nock": "^12.0.3",
    "rewire": "^5.0.0",
    "shx": "^0.3.2",
